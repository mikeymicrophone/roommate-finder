<% form_for @chore do |f| %>
	<%= f.text_field :name %>
	<%= f.label :name %><br>
	<%= f.select :manager_id, Manager.all.map { |m| [m.name, m.id] } %>
	<%= f.label :manager_id %><br>
	<%= f.select :unit_id, Unit.all.map { |u| [u.name, u.id] } %>
	<%= f.label :unit_id %><br>
	<%= f.text_area :description %>
	<%= f.label :description %><br>
	<%= f.submit %>
<% end %>